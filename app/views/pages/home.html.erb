<div class="container">
  <div class="row">
    <div class="col-md-11">
      <input type="text" class="form-control" placeholder="O que você está procurando?" />
    </div>
    <div class="col-md-1">
      <button type="submit" class="btn btn-default">Submit</button>
    </div>
  </div>
  <div class="row galeria">

    <% for dica in @dicas %>
    <div class="col-sm-6 col-md-4 grow">
      <div class="thumbnail">
        <img class="img-responsive" src="<%= dica.imagem.url(:medium) %>" alt="Rails Girls">
        <div class="caption">
          <h3><%= dica.titulo %></h3>
          <p><%= dica.descricao %></p>
          <p><a href="#" class="btn btn-primary" role="button">Button</a> <a href="#" class="btn btn-default" role="button">Button</a></p>
        </div>
      </div>
    </div>
    <% end %>

  </div>
</div>